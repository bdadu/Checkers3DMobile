{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "jsx": "react-jsx",
  "esModuleInterop": true,
  "allowSyntheticDefaultImports": true,
    "paths": {
      "@/*": ["./*"],
      "navigation/*": ["navigation/*"]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ]
}