{
  "expo": {
    "name": "Checkers 3D",
    "slug": "checkers-3d",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "checkers3dios",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": false,
    "description": "Experience classic checkers in stunning 3D! Play against AI with multiple difficulty levels in this beautiful three-dimensional board game.",
    "keywords": ["checkers", "3d", "board game", "strategy", "ai", "chess"],
    "primaryColor": "#8B4513",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.bdadu.checkers3d"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.bdadu.checkers3d"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-asset",
      "expo-router"
    ],
    "experiments": {}
  }
}